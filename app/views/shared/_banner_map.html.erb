<div class="banner-profil">
  <div style="width: 100%; height: 200px;"
      data-controller="map"
      data-map-markers-value="<%= @markers.to_json %>"
      data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>">
  </div>
  <%= link_to profile_show_path(@event.user) do %>
    <%= cl_image_tag @event.user.photo.key, width: 100, height: 100, crop: :thumb, gravity: :face, class: 'avatar-large' %>
  <% end %>
</div>
